{
  "name": "@aspire/electron",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "../../dist-electron/main.js",
  "scripts": {
    "build": "pnpm build:renderer && pnpm build:electron",
    "build:electron": "tsc -p tsconfig.json && node -e \"const fs=require('fs'); fs.renameSync('../../../dist-electron/main.js','../../../dist-electron/main.cjs'); fs.renameSync('../../../dist-electron/preload.js','../../../dist-electron/preload.cjs');\"",
    "build:renderer": "vite build --config vite.config.ts"
  },
  "dependencies": {
    "electron": "^39.2.7",
    "@aspire/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "typescript": "^5.9.3"
  }
}
